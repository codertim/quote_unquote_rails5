
<header class="navbar navbar-fixed-top navbar-inverse container-fluid">
  <div class="container-fluid-DISABLED">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#target-navbar-collapse" aria-expanded="false">
		    <span class="sr-only">Toggle navigation</span>
		    <span class="icon-bar"></span>
		    <span class="icon-bar"></span>
		    <span class="icon-bar"></span>
      </button>
      <%= link_to "Quote Unquote", root_path, id: "logo", class: "navbar-brand" %>
    </div>
    <nav class="collapse navbar-collapse" id="target-navbar-collapse">
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Home", root_path %></li>
